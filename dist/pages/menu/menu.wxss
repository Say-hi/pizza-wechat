page {
  width: 100%;
  height: 100%;
}
.tab {
  display: flex;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  justify-content: space-around;
  align-items: center;
  background-color: #fff;
  height: 5%;
}
.tab view {
  flex: 1;
  height: 100%;
  text-align: center;
}
.tab .show {
  border-bottom: 1rpx solid #f30;
}
.t-zw {
  height: 5%;
}
.coupon {
  background-color: #f7f7f7;
  width: 100%;
  height: 18%;
  box-sizing: border-box;
  padding: 20rpx 10rpx;
}
.coupon .coupon-in {
  display: flex;
  flex-wrap: nowrap;
}
.coupon .coupon-in view {
  word-break: keep-all;
}
.coupon .coupon-in .c {
  display: flex;
  margin: 0 20rpx;
  padding: 20rpx 10rpx;
  border-radius: 10rpx;
  background: linear-gradient(90deg, #ff7305, #ff623a);
  position: relative;
}
.coupon .coupon-in .c:before,
.coupon .coupon-in .c:after {
  content: '';
  width: 15rpx;
  height: 15rpx;
  position: absolute;
  bottom: 50%;
  background-color: #f7f7f7;
  border-radius: 50%;
}
.coupon .coupon-in .c:before {
  left: 0;
  transform: translate(-50%, 0%);
}
.coupon .coupon-in .c:after {
  right: 0;
  transform: translate(50%, 0%);
}
.coupon .coupon-in .c .c-left,
.coupon .coupon-in .c .c-right {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.coupon .coupon-in .c .c-left .main-text-color,
.coupon .coupon-in .c .c-right .main-text-color {
  padding: 7rpx;
  background-color: #ffcec2;
  border-radius: 10rpx;
}
.coupon .coupon-in .c .c-right {
  margin-left: 20rpx;
}
.coupon .coupon-in .c .c-right .main-text-color {
  margin-top: 40rpx;
}
.menu-list {
  height: 77%;
  padding: 0 40rpx;
  background-color: #fff;
  box-sizing: border-box;
}
.menu-list .m-l {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 0;
}
.menu-list .m-l image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 10rpx;
}
.menu-list .m-l .m-l-c {
  flex: 1;
  margin-left: 20rpx;
}
.menu-list .m-l .m-l-c .f22 {
  margin-top: 10rpx;
}
.menu-list .m-l .m-l-c .ico {
  background-color: #f30;
  padding: 0 6rpx;
  margin-right: 10rpx;
}
.menu-list .m-l .choose-count {
  display: flex;
}
.menu-list .m-l .choose-size {
  background-color: #f30;
  border-radius: 44rpx;
  line-height: 44rpx;
  padding: 0 20rpx;
}
.menu-list .m-l .m-l-r {
  display: flex;
  align-items: center;
}
.menu-list .m-l .m-l-r .order-btn,
.menu-list .m-l .m-l-r .order-num {
  width: 50rpx;
  height: 50rpx;
  line-height: 45rpx;
  text-align: center;
  border-radius: 50%;
  background-color: #f30;
  margin: 0 10rpx;
  vertical-align: middle;
  font-size: 50rpx;
}
.menu-list .m-l .m-l-r .order-num {
  background: #fff;
  margin: 0;
  width: inherit;
  font-size: 24rpx;
}
.menu-list .zw {
  height: 90rpx;
}
.m-bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 88rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.m-bottom .icon-gouwuche {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: #f30;
  text-align: center;
  line-height: 100rpx;
  font-size: 50rpx;
  position: relative;
  transform: translateY(-50%);
  margin-left: 20rpx;
}
.m-bottom .icon-gouwuche text {
  position: absolute;
  top: 0;
  right: 0;
  min-width: 13rpx;
  height: 33rpx;
  padding: 0 10rpx;
  border-radius: 33rpx;
  line-height: 33rpx;
  transform: translateX(25%);
  background-color: #ff9000;
}
.m-bottom .m-money {
  flex: 1;
  padding-right: 30rpx;
  text-align: right;
}
.m-bottom .confirm {
  height: 100%;
  width: 220rpx;
  line-height: 88rpx;
  background-color: #f30;
}
.mask-in {
  width: 647rpx;
  border-radius: 10rpx;
  padding: 20rpx;
}
.mask-in .mask-title {
  position: relative;
}
.mask-in .close {
  position: absolute;
  top: 50%;
  right: 0;
  color: #fff !important;
  background-color: #000;
  transform: translateY(-50%);
  border-radius: 50%;
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
}
.mask-in .size-w {
  display: flex;
  flex-wrap: wrap;
  width: 100%;
}
.mask-in .size {
  padding: 10rpx 20rpx;
  background-color: #e1e1e1;
  color: #666 !important;
  border-radius: 5rpx;
  margin: 10rpx 5rpx;
  word-break: keep-all;
}
.mask-in .size-choose {
  background: #f30;
  color: #fff !important;
}
.mask-in .choose-count-w {
  display: flex;
  justify-content: space-between;
}
.mask-in .choose-count-w text {
  color: #333;
}
.mask-in .choose-count {
  display: flex;
  align-items: center;
}
.mask-in .order-btn,
.mask-in .order-num {
  width: 44rpx;
  height: 44rpx;
  line-height: 44rpx;
  text-align: center;
  border-radius: 50%;
  background-color: #f30;
  margin: 0 10rpx;
  vertical-align: middle;
  font-size: 50rpx;
}
.mask-in .order-num {
  background: #fff;
  margin: 0;
  width: inherit;
  font-size: 24rpx;
}
.mask-in .m-confirm {
  margin-top: 40rpx;
  border-top: 2rpx solid #f7f7f7;
  padding-top: 20rpx;
  display: flex;
  justify-content: space-between;
}
.mask-in .m-confirm .lh56 {
  border-radius: 5rpx;
  color: #fff;
  font-size: 30rpx;
  padding: 0 20rpx;
}
.mask-in .m-confirm .add-car {
  background: #f63;
}
.mask-in .m-confirm .buy-now {
  background-color: #f30;
}
